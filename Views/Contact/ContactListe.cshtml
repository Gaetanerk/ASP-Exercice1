@model List<Contact>
<h1 class="text-center my-4">@ViewBag.ContactListe</h1>
<h2 class="text-center my-4">@ViewData["emails"]</h2>
<div class="container">
    <div class="row">
        @foreach (var contact in Model)
        {
            <div class="col-md-4 mb-4">
                <div class="card">
                    <div class="card-body">
                        <h2 class="card-title">@contact.Name @contact.FirstName</h2>
                        <p class="card-text">@contact.DetailContact.Email</p>
                    </div>
                </div>
            </div>
        }
    </div>
</div>